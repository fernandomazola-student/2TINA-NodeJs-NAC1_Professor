<html>
  <body>
  <h1>Clientes</h1>

    <table>
        <tr>
          <td>ID</td>
          <td>Nome</td>
          <td>Sobrenome</td>
          <td>Rg</td>

        </tr>


       <% for(var i=0; i < clientes.length; i++) { %>
        <tr>
          <td><%= clientes[i].id_cliente %></td>
          <td><%= clientes[i].nome %></td>
          <td><%= clientes[i].sobrenome %></td>
          <td><%= clientes[i].rg %></td>

        </tr>

        <% } %>
    </table>


  </body>
</html>
